{
  "name": "codemind-enhanced-cli",
  "version": "2.0.0",
  "description": "Smart Claude Code CLI with intelligent tool selection, token optimization, and real-time quality monitoring",
  "main": "dist/index.js",
  "bin": {
    "codemind": "./bin/codemind.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testMatch='**/unit/**/*.test.ts'",
    "test:integration": "jest --testMatch='**/integration/**/*.test.ts'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:unit && npm run test:integration",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "typecheck": "tsc --noEmit",
    "mcp:dev": "tsx src/mcp/server.ts",
    "mcp:build": "tsc && node dist/mcp/server.js",
    "self-improve": "tsx src/cli/codemind.ts self-improve",
    "dogfood": "tsx src/cli/codemind.ts dogfood",
    "analyze": "tsx src/cli/analyze.ts",
    "init": "tsx src/cli/init.ts",
    "codemind": "node dist/cli/codemind-unified.js",
    "codemind:dev": "npm run build && node dist/cli/codemind-unified.js",
    "codemind:cycle": "tsx src/cli/codemind-cycle-simple.ts",
    "codemind:cycle:build": "npm run build && node dist/cli/codemind-cycle-simple.js",
    "codemind:orchestrated": "tsx src/cli/codemind-orchestrated.ts",
    "codemind:preview": "tsx src/cli/codemind-orchestrated.ts preview",
    "codemind:branches": "tsx src/cli/codemind-orchestrated.ts branches",
    "codemind:old": "tsx src/cli/codemind.ts",
    "codemind:enhance": "node dist/cli/codemind-context-enhancer.js",
    "codemind:enhance:dev": "npm run build && node dist/cli/codemind-context-enhancer.js",
    "codemind-enhanced": "tsx src/cli/codemind-enhanced-v2.ts",
    "multi-llm": "tsx src/cli/multi-llm-cli.ts",
    "marketplace": "tsx src/cli/marketplace.ts",
    "start": "npm run build && node dist/api/server.js",
    "start:dev": "npm run build && node dist/api/server.js --dev",
    "start:postgres": "DB_TYPE=postgresql npm run start:dev",
    "dashboard": "echo \"⚠️  Dashboard should run in Docker. Use: docker-compose --profile dev up dashboard\" && echo \"Or run: .\\scripts\\start-dashboard-docker.ps1\"",
    "dashboard:dev": "echo \"⚠️  Dashboard should run in Docker. Use: docker-compose --profile dev up dashboard\" && echo \"Or run: .\\scripts\\start-dashboard-docker.ps1\"",
    "dashboard:local": "node src/dashboard/server.js --dev",
    "orchestrator": "tsx src/orchestration/orchestrator-server.ts",
    "orchestrator:dev": "tsx src/orchestration/orchestrator-server.ts --dev",
    "role-terminal": "tsx src/orchestration/role-terminal-worker.ts",
    "role-terminal:dev": "tsx src/orchestration/role-terminal-worker.ts --dev",
    "init-project": "node scripts/init-project.js",
    "init-databases": "node scripts/initialize-all-databases.js",
    "init-master": "node scripts/master-database-init.js",
    "codemind-cli": "tsx src/cli/codemind-cli.ts",
    "setup:test-db": "tsx tests/setup-test-database.ts",
    "test:setup": "npm run setup:test-db"
  },
  "keywords": [
    "claude-code",
    "cli",
    "intelligent-tool-selection",
    "token-optimization",
    "code-quality",
    "real-time-monitoring",
    "context-optimization",
    "duplication-detection",
    "ai-enhancement",
    "machine-learning",
    "typescript"
  ],
  "author": "CodeMind Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@babel/parser": "^7.23.4",
    "@babel/traverse": "^7.23.4",
    "@babel/types": "^7.23.4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/inquirer": "^9.0.9",
    "@types/lz-string": "^1.5.0",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.1.12",
    "@types/redis": "^4.0.11",
    "better-sqlite3": "^12.2.0",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "commander": "^11.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "duckdb": "^1.1.3",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "fast-glob": "^3.3.2",
    "inquirer": "^12.9.4",
    "ioredis": "^5.3.2",
    "lucide-react": "^0.542.0",
    "lz-string": "^1.5.0",
    "mongodb": "^6.3.0",
    "neo4j-driver": "^5.28.1",
    "node-cron": "^4.2.1",
    "ora": "^7.0.1",
    "pg": "^8.11.3",
    "react": "^19.1.1",
    "redis": "^5.8.2",
    "socket.io": "^4.7.5",
    "sqlite3": "^5.1.6",
    "tree-kill": "^1.2.2",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.0",
    "@types/babel__traverse": "^7.20.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.11",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.1.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/codemind-enhanced-cli.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/codemind-enhanced-cli/issues"
  },
  "homepage": "https://github.com/your-org/codemind-enhanced-cli#readme"
}
