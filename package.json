{
  "name": "codemind-enhanced-cli",
  "version": "2.0.0",
  "description": "Smart Claude Code CLI with intelligent tool selection, token optimization, and real-time quality monitoring",
  "main": "dist/index.js",
  "bin": {
    "codemind": "./bin/codemind.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/cli/codemind-cli.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "typecheck": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "codemind": "npm run build && npm run start"
  },
  "keywords": [
    "claude-code",
    "cli",
    "intelligent-tool-selection",
    "token-optimization",
    "code-quality",
    "real-time-monitoring",
    "context-optimization",
    "duplication-detection",
    "ai-enhancement",
    "machine-learning",
    "typescript"
  ],
  "author": "CodeMind Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@babel/parser": "^7.23.4",
    "@babel/traverse": "^7.23.4",
    "@babel/types": "^7.23.4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/inquirer": "^9.0.9",
    "@types/lz-string": "^1.5.0",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.1.12",
    "@types/redis": "^4.0.11",
    "better-sqlite3": "^12.2.0",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "commander": "^11.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "duckdb": "^1.1.3",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "fast-glob": "^3.3.2",
    "glob": "^11.0.3",
    "inquirer": "^12.9.4",
    "ioredis": "^5.3.2",
    "lucide-react": "^0.542.0",
    "lz-string": "^1.5.0",
